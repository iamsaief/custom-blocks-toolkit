{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "sf-blocks/fancy-header",
	"title": "Fancy Header",
	"category": "text",
	"icon": "block-default",
	"description": "Shows bunch of dummy text",
	"keywords": ["alert", "message"],
	"version": "1",
	"textdomain": "sf-blocks",
	"script": "file:./index.js",
	"style": ["file:./style.css"],
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "h2"
		},
		"textColor": {
			"type": "string",
			"default": "#000000"
		}
	}
}
